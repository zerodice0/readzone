{
  "name": "readzone",
  "version": "1.0.0",
  "description": "독서 후 의견을 공유하는 독서 전용 커뮤니티 SNS 플랫폼",
  "private": true,
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:frontend": "turbo dev --filter=@readzone/frontend",
    "dev:backend": "turbo dev --filter=@readzone/backend",
    "build": "turbo build",
    "build:frontend": "turbo build --filter=@readzone/frontend",
    "build:backend": "turbo build --filter=@readzone/backend",
    "lint": "turbo lint",
    "lint:frontend": "turbo lint --filter=@readzone/frontend",
    "lint:backend": "turbo lint --filter=@readzone/backend",
    "type-check": "turbo type-check",
    "type-check:frontend": "turbo type-check --filter=@readzone/frontend",
    "type-check:backend": "turbo type-check --filter=@readzone/backend",
    "test": "turbo test",
    "test:frontend": "turbo test --filter=@readzone/frontend",
    "test:backend": "turbo test --filter=@readzone/backend",
    "db:generate": "turbo db:generate --filter=@readzone/backend",
    "db:migrate": "turbo db:migrate --filter=@readzone/backend",
    "db:reset": "turbo db:reset --filter=@readzone/backend",
    "db:seed": "turbo db:seed --filter=@readzone/backend",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.131.27",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "독서",
    "커뮤니티",
    "SNS",
    "독후감",
    "리뷰",
    "책",
    "도서"
  ],
  "author": "ReadZone Team",
  "license": "MIT"
}
